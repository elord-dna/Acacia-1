{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [],
    "rules":{
        "indent": ["error", "tab"],
        "eqeqeq": "off",
        "no-underscore-dangle":"warn",
        "linebreak-style": ["off", "windows"],
        "no-param-reassign": "off",
        "consistent-return": "off"
    },
    "globals":{
		"describe": false,
		"it": false
	}
}
